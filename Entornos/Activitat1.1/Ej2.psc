Algoritmo Ej2
	Escribir "Introduce el primer número:"
    Leer num1
    Escribir "Introduce el segundo número:"
    Leer num2
    suma <- num1 + num2
    resta <- num1 - num2
    multi <- num1 * num2
    division <- num1 / num2
    Escribir "La suma es:", suma
    Escribir "La resta es:", resta
    Escribir "El multiplicación es:", multi
    Escribir "La división es:", division
FinAlgoritmo
